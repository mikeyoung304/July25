{
  "name": "@rebuild/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for Rebuild 6.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "default": "./dist/config/index.js"
    },
    "./config/browser": {
      "types": "./dist/config/browser.d.ts",
      "import": "./dist/config/browser.esm.js",
      "require": "./dist/config/browser.js",
      "default": "./dist/config/browser.js"
    },
    "./config/node": {
      "types": "./dist/config/node.d.ts",
      "default": "./dist/config/node.js"
    },
    "./config/kds": {
      "types": "./dist/config/kds.d.ts",
      "default": "./dist/config/kds.js"
    },
    "./constants/business": {
      "types": "./dist/constants/business.d.ts",
      "import": "./dist/constants/business.esm.js",
      "require": "./dist/constants/business.js",
      "default": "./dist/constants/business.js"
    },
    "./*": null
  },
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && node fix-esm.js",
    "typecheck": "tsc --noEmit",
    "typecheck:quick": "tsc --noEmit",
    "lint": "eslint config/index.ts config/browser.ts config/node.ts",
    "test:quick": "echo \"no quick tests\""
  },
  "devDependencies": {
  },
  "dependencies": {
    "typescript": "^5.3.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.1",
    "zod": "^3.25.76"
  }
}
