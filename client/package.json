{
  "name": "restaurant-os-client",
  "private": true,
  "version": "6.0.3",
  "scripts": {
    "dev": "vite",
    "dev:clean": "npx kill-port 5173 && vite",
    "verify": "node verify-setup.js",
    "build": "cross-env ROLLUP_SKIP_NODEJS_NATIVE=true ROLLUP_DISABLE_NATIVE=true vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview --port 4173",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:memory": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' vitest run",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' VITEST_COVERAGE=false vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze": "vite build --mode analyze",
    "test:quick": "vitest run --bail=1 --reporter=dot --silent"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@square/web-payments-sdk-types": "^1.76.1",
    "@supabase/supabase-js": "^2.50.5",
    "@types/react-window": "^1.8.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.3",
    "react-window": "^1.8.11",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "cross-env": "^7.0.3",
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "kill-port": "^2.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "puppeteer": "^23.11.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.11",
    "terser": "^5.43.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "5.4.19",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3",
    "zod": "^3.25.76"
  }
}
