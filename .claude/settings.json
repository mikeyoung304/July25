{
  "project": {
    "name": "rebuild-6.0",
    "type": "restaurant-os",
    "version": "6.0.0",
    "stack": ["react", "typescript", "vite", "express", "supabase"]
  },
  "architecture": {
    "backend": "unified",
    "port": 3001,
    "microservices": false,
    "structure": {
      "frontend": "client/",
      "backend": "server/",
      "shared": "shared/"
    }
  },
  "workflow": {
    "autoTest": true,
    "strictTypeCheck": true,
    "preCommitChecks": ["test", "lint", "typecheck"],
    "commands": {
      "dev": "npm run dev",
      "test": "npm test",
      "lint": "npm run lint:fix",
      "typecheck": "npm run typecheck"
    }
  },
  "subagents": {
    "preferredAgents": [
      "typescript-debugger",
      "vite-builder",
      "react-optimizer"
    ],
    "autoDelegate": {
      "typeErrors": "typescript-debugger",
      "buildIssues": "vite-builder",
      "performance": "react-optimizer"
    }
  },
  "multiTenancy": {
    "enabled": true,
    "contextProvider": "RestaurantContext",
    "idField": "restaurant_id"
  },
  "aiIntegration": {
    "location": "server/src/ai/",
    "websocket": true,
    "voiceEnabled": true
  },
  "qualityGates": {
    "mandatory": ["test", "typecheck"],
    "coverage": {
      "statements": 60,
      "branches": 50,
      "functions": 60,
      "lines": 60
    }
  }
}