{
  "name": "restaurant-os-server",
  "version": "6.0.3",
  "description": "Backend API for Grow Fresh Local Food Restaurant OS",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "echo 'Build step skipped - using tsx in production'",
    "build:strict": "tsc -p tsconfig.prod.json",
    "start": "tsx src/server.ts",
    "start:dist": "node dist/server.js",
    "start:prod": "npm run build && node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "bash scripts/test-integration.sh",
    "test:voice": "vitest run tests/voice-integration.test.ts",
    "seed": "tsx scripts/seed-menu.ts",
    "seed:menu": "tsx scripts/seed-menu.ts",
    "migrate": "tsx scripts/migrate.ts",
    "migrate:run": "tsx scripts/migrate.ts",
    "lint": "cd .. && npm run lint:fix -- server/src",
    "lint:fix": "cd .. && npm run lint:fix -- server/src",
    "typecheck": "cd .. && npm run typecheck",
    "start:all": "./scripts/start-all.sh",
    "init:menu": "tsx scripts/init-menu-context.ts",
    "check:integration": "tsx scripts/integration-check.ts",
    "db:migrate": "node scripts/run-migrations.js",
    "db:check": "node scripts/run-migrations.js check",
    "seed:tables": "tsx scripts/seed-tables-simple.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.39.7",
    "bcryptjs": "^3.0.2",
    "chalk": "5.3.0",
    "cookie-parser": "^1.4.7",
    "cors": "2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "16.4.1",
    "express": "^4.21.2",
    "express-prom-bundle": "^8.0.0",
    "express-rate-limit": "7.1.5",
    "helmet": "7.1.0",
    "joi": "17.12.0",
    "jsonwebtoken": "9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cache": "5.1.2",
    "openai": "^4.104.0",
    "prom-client": "^15.1.3",
    "square": "^43.0.1",
    "typescript": "5.3.3",
    "uuid": "^9.0.1",
    "winston": "3.11.0",
    "ws": "^8.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "20.11.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@vitest/ui": "1.2.0",
    "supertest": "^7.1.3",
    "tsx": "^4.20.5",
    "vitest": "1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "restaurant",
    "api",
    "express",
    "typescript",
    "supabase"
  ],
  "author": "Macon AI Solutions",
  "license": "PROPRIETARY"
}
